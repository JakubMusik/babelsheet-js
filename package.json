{
  "name": "babelsheet-node",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "start-dev": "pm2-dev src/index.ts",
    "format": "prettier --write 'src/**/*.ts'",
    "postinstall": "pm2 install typescript",
    "type-check": "tsc --noEmit",
    "build": "tsc"
  },
  "author": "The Software House",
  "license": "MIT",
  "dependencies": {
    "awilix": "^3.0.6",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "morgan": "^1.9.0",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/cors": "^2.8.3",
    "@types/express": "^4.11.1",
    "@types/glob": "^5.0.35",
    "@types/jest": "^22.2.0",
    "@types/node": "^9.4.7",
    "@types/supertest": "^2.0.4",
    "@types/winston": "^2.3.8",
    "jest": "^22.4.3",
    "pm2": "^2.10.1",
    "prettier": "^1.11.1",
    "supertest": "^3.0.0",
    "ts-jest": "^22.4.2",
    "typescript": "^2.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
